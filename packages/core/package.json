{
  "name": "@meshsdk/core",
  "description": "Rapidly build Web3 apps on the Cardano Blockchain.",
  "homepage": "https://meshjs.dev",
  "author": "MeshJS",
  "version": "1.5.24",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeshJS/mesh.git"
  },
  "bugs": {
    "url": "https://github.com/MeshJS/mesh/issues"
  },
  "keywords": [
    "blockchain",
    "cardano",
    "plutus"
  ],
  "main": "dist/core.cjs",
  "module": "dist/core.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "vite build && pnpm run docs",
    "dev": "vite build --watch",
    "docs": "typedoc src/index.ts --json ../../apps/mesh-docs/src/data/api.json"
  },
  "exports": {
    ".": {
      "import": "./dist/core.js",
      "require": "./dist/core.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "11.1.6",
    "rollup": "4.18.0",
    "tslib": "2.6.3",
    "typedoc": "0.25.13",
    "vite": "5.2.13",
    "vite-plugin-wasm": "3.3.0",
    "vitest": "1.6.0"
  },
  "dependencies": {
    "@emurgo/cardano-message-signing-asmjs": "1.0.1",
    "@emurgo/cardano-serialization-lib-asmjs": "11.5.0",
    "@emurgo/cip14-js": "3.0.1",
    "axios": "1.7.2",
    "bip39": "3.1.0",
    "json-bigint": "1.0.0",
    "nanoid": "5.0.7",
    "zod": "3.23.8"
  }
}